{
  "expo": {
    "name": "Buzz",
    "slug": "Buzz",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splashnew.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "permissions": [
        "INTERNET"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.CollegeKeDin"
    },
    "web": {
      "favicon": "./assets/finallogo.png"
    },
    "plugins": [
      [
        "./plugin/withDisableForcedDarkModeAndroid.js",
        {}
      ]
    ]
  }
}

